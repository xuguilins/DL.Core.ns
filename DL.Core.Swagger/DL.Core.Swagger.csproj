    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>支持多个xml文件,以英文逗号分割，需在配置文件配置</Description>
    <Authors>余温</Authors>
    <Product>DL.Core.Swagger</Product>
